# Deployment Configuration
# Copy this file to .deploy.env and fill in your actual values
# DO NOT commit .deploy.env to version control

# VPS Configuration
VPS_IP=your.vps.ip.address
VPS_USER=root

# SSH Configuration (optional - leave empty to use default ~/.ssh/id_rsa)
SSH_KEY_PATH=

# Supabase Configuration (for deployment scripts that need it)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here
JWT_SECRET=your-jwt-secret-here
